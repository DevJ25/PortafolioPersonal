<script>
  document.addEventListener('DOMContentLoaded', () => {
    const tabs = document.querySelectorAll('[data-tab-trigger]');
    const tabContents = document.querySelectorAll('[data-tab-content]');

    tabs.forEach(tab => {
      tab.addEventListener('click', () => {
        const target = tab.getAttribute('data-tab-trigger');

        // Update active tab
        tabs.forEach(t => t.classList.remove('active', 'bg-white/20', 'text-emerald-300', 'border-emerald-300', 'scale-105'));
        tab.classList.add('active', 'bg-white/20', 'text-emerald-300', 'border-emerald-300', 'scale-105');

        // Show/hide content
        tabContents.forEach(content => {
          const tabContent = content.getAttribute('data-tab-content');
          if (tabContent === target) {
            content.classList.remove('hidden');
          } else {
            content.classList.add('hidden');
          }
        });
      });
    });
  });
</script>

<style>
  [data-tab-trigger].active {
    /* Extra visual feedback for active tab if needed */
    box-shadow: 0 4px 24px 0 rgba(16, 185, 129, 0.15);
    outline: none;
  }
</style>