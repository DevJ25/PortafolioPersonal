@import '//fonts.googleapis.com/css?family=Cutive';

$font-family-base: 'Cutive', serif;
$font-size-root: 14px;
$font-size-base: 1rem;
$line-height-base: 1.4;

$color-black: #222;
$color-white: #ededed;
$color-accent: #2b6bc9; 

$spacing: 2rem;
$spacing-large: $spacing * 3;




.scene {
  padding: $spacing-large $spacing;
  max-width: 800px;
  margin: 0 auto;
}

.actor {
  font-size:20px; // Reducido ligeramente para textos más largos
  display: flex;
  color:gray;
  margin-bottom: $spacing;
  align-items: flex-start; // Alinea el prefijo y el contenido arriba

  &:last-of-type {
    margin-bottom: $spacing-large;
  }
}

.actor__prefix {
  font-weight: bold;
  margin-right: 0.5rem;
  min-width: 120px; // Ajusta el ancho para el prefijo "Estudiante:"
  text-align: right; // Alinea el prefijo a la derecha
  color: $color-accent; // Color para el prefijo del actor
}

.actor__content {
  flex-grow: 1;
}

@keyframes blink {
  from { opacity: 0; }
  to { opacity: 1; }
}

.actor__content--typing::after {
  content: '|';
  animation: blink 500ms infinite;
}

</style>


<script src="https://unpkg.com/theaterjs@latest/dist/theater.umd.js"></script>

<script is:inline>
document.addEventListener('DOMContentLoaded', function() {
  // Diálogo personalizado para estudiante de Ingeniería de Sistemas e Informática


  const dialog = [
  {actor: 'estudiante', text: "Estudiante apasionado de Ingeniería de Sistemas e Informática" },
  {actor: 'estudiante', text: "Desarrollo soluciones digitales que impactan positivamente" },
  {actor: 'estudiante', text: "Especial interés en desarrollo web y ciberseguridad" },
  {actor: 'estudiante', text: "Participante activo en hackathons y proyectos tecnológicos" },
  {actor: 'estudiante', text: "Aprendiz constante de nuevas tecnologías y frameworks" },
  {actor: 'estudiante', text: "Creo en el poder de la programación para transformar ideas" },
  {actor: 'estudiante', text: "Comprometido con el aprendizaje continuo y crecimiento profesional" },
  {actor: 'estudiante', text: "Futuro ingeniero innovador con visión de impacto social" }
];
  const speed = 30; // velocidad de escritura (ms por letra)
  const pause = 1000; // pausa entre frases

  const seniorEl = document.getElementById('senior');
  const estudianteEl = document.getElementById('estudiante');

  let i = 0;

  function typeText(actor, text, el, cb) {
    el.innerHTML = '';
    let j = 0;
    function type() {
      el.innerHTML = text.slice(0, j) + '<span class="actor__content--typing">|</span>';
      if (j < text.length) {
        j++;
        setTimeout(type, speed);
      } else {
        el.innerHTML = text; // quitar el cursor
        setTimeout(cb, pause);
      }
    }
    type();
  }

  function next() {
    if (i >= dialog.length) {
      i = 0; // Si quieres que se repita, si no, elimina esta línea
      // return; // Si quieres que se detenga al final, descomenta esta línea
    }
    const { actor, text } = dialog[i];
    if (actor === 'estudiante') {
      typeText(actor, text, seniorEl, () => {
        estudianteEl.innerHTML = '';
        i++;
        setTimeout(next, 300);
      });
    } else {
      typeText(actor, text, estudianteEl, () => {
        seniorEl.innerHTML = '';
        i++;
        setTimeout(next, 300);
      });
    }
  }

  next();

  // Botón de copiar (mantengo tu funcionalidad original)
  document.getElementById('copyButton')?.addEventListener('click', () => {
    const textToCopy = `RedC24990@gmail.com`;
    navigator.clipboard.writeText(textToCopy).then(() => {
      let btn = document.getElementById('copyButton');
      if (!btn) return;
      btn.innerHTML = '✓';
